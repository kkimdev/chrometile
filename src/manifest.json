{
  "manifest_version": 3,
  "name": "Window shortcut placer for Chrome OS",
  "description": "Window shortcut placer for Chrome OS",
  "version": "0.7",
  "icons": {"128": "icon.png"},

  "permissions": [
    "storage",
    "system.display"
  ],

  "options_ui": {
    "page": "options/options.html"
  },

  "background" : {
    "service_worker": "background2.js"
  },

  "commands": {
    "99001-place-1":        {"description": "1"},
    "99002-place-2":        {"description": "2"},
    "99003-place-3":        {"description": "3"},
    "99004-place-4":        {"description": "4", "suggested_key": { "chromeos": "Alt+A"}},
    "99005-place-5":        {"description": "5", "suggested_key": { "chromeos": "Alt+S"}},
    "99006-place-6":        {"description": "6", "suggested_key": { "chromeos": "Alt+D"}},
    "99007-place-7":        {"description": "7"},
    "99008-place-8":        {"description": "8"},
    "99009-place-9":        {"description": "9"}
  }
}
